{
  "name": "xuanxuan",
  "productName": "喧喧",
  "version": "2.3.0",
  "description": "企业即时通讯平台",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 ./tools/server.js",
    "build-main": "cross-env NODE_ENV=production webpack --config ./tools/webpack.config.electron.js --progress",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./tools/webpack.config.production.js --progress",
    "build-renderer-debug": "cross-env NODE_ENV=debug webpack --config ./tools/webpack.config.debug.js --progress",
    "build-main-debug": "cross-env NODE_ENV=debug webpack --config ./tools/webpack.config.electron.js --progress",
    "build": "npm run build-main && npm run build-renderer",
    "build-debug": "npm run build-main-debug && npm run build-renderer-debug",
    "build-browser": "cross-env NODE_ENV=production webpack --config ./tools/webpack.config.browser.js --progress",
    "start": "npm run hot-server-browser",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./app/main.development",
    "start-hot-fast": "cross-env HOT=1 NODE_ENV=development SKIP_INSTALL_EXTENSIONS=1 electron ./app/main.development",
    "postinstall": "npm run package -- --skipbuild && concurrently \"electron-builder install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "package": "node --max_old_space_size=2096 ./build/package.js",
    "package-beta": "npm run package -- --beta",
    "package-debug": "npm run package -- --debug",
    "package-mac": "npm run package -- --platform=mac --arch=x64",
    "package-mac-debug": "npm run package -- --platform=mac --arch=x64 --debug",
    "package-win": "npm run package -- --platform=win --arch=x64",
    "package-win-32": "npm run package -- --platform=win --arch=x32",
    "package-win-all": "npm run package -- --platform=win --arch=all",
    "package-linux": "npm run package -- --platform=linux --arch=x64",
    "package-linux-32": "npm run package -- --platform=linux --arch=x32",
    "package-win-debug": "npm run package -- --platform=win --arch=x64 --debug",
    "package-all": "npm run package -- --platform=all --arch=all --clean && npm run package -- --platform=mac,win --arch=x64 --debug",
    "package-all-beta": "npm run package -- --platform=all --arch=all --beta --clean && npm run package -- --platform=mac,win --arch=x64 --debug --beta",
    "package-browser": "npm run package -- --platform=browser",
    "hot-server-browser": "npm run hot-server -- --target=browser",
    "docs": "esdoc",
    "eslint": "eslint ./app"
  },
  "build": {
    "extends": "./build/electron-builder.json"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron",
    "eslint": "./node_modules/.bin/eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easysoft/xuanxuan.git"
  },
  "author": {
    "name": "easysoft",
    "email": "sunhao@cnezsoft.com"
  },
  "license": "ZPL 1.2",
  "bugs": {
    "url": "https://github.com/easysoft/xuanxuan/issues"
  },
  "keywords": [
    "im",
    "message",
    "electron",
    "react",
    "webpack",
    "react-hot"
  ],
  "homepage": "http://xuan.im",
  "company": "QingDao Nature Easy Soft Network Technology Co,LTD.",
  "devDependencies": {
    "@babel/core": "^7.23.3",
